{
  "lab_name": "DVWA Lab",
  "target": "http://localhost:8080",
  "scope": {
    "include": [
      "http://localhost:8080/*"
    ],
    "exclude": []
  },
  "environment": "lab",
  "authorization": {
    "auth_file": "authorizations/auth_dvwa_lab.json",
    "auth_type": "lab"
  },
  "scan_settings": {
    "rate_limit": 10,
    "max_concurrent": 5,
    "timeout": 10
  },
  "vulnerability_types": [
    "xss",
    "sqli",
    "idor",
    "lfi",
    "command_injection"
  ]
}
